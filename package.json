        "test": "vitest"
    },
    "devDependencies": {
        "@microsoft/api-extractor": "^7.43.0",
        "@types/node": "^20.11.30",
        "rollup-plugin-node-externals": "^7.1.0",
        "tsup": "^8.0.2",
        "typescript": "^4.5.5",
